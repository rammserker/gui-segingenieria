			<w:r w:rsidRPr="002B14E2">
				<w:rPr>
					<w:rFonts w:cstheme="minorHAnsi"/>
				</w:rPr>
				<w:t xml:space="preserve">de una presión mayor a una menor, las moléculas que escapan a gran velocidad </w:t>
			</w:r>
			<w:r w:rsidR="00416727" w:rsidRPr="002B14E2">
				<w:rPr>
					<w:rFonts w:cstheme="minorHAnsi"/>
				</w:rPr>
				<w:t>chocan con</w:t>
